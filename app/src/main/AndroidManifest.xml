<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.banat_travel_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/b_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainPart.MainActivity" />
        <activity android:name=".StartingPart.Activity.ForgotPasswordActivity" />
        <activity android:name=".StartingPart.Activity.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StartingPart.Activity.LoginActivity" />
        <activity android:name=".ShoppingCartDetailsFromDate" />
        <activity android:name=".ShoppingCartDetails" />
        <activity android:name=".CountySelector" />
        <activity android:name=".AreaSelector" />
        <activity android:name=".ActivityPresentation" />
        <activity android:name=".Activities" />
        <activity android:name=".StartingPart.Activity.RegisterActivity" />
        <activity android:name=".ShoppingCart" />
        <activity android:name=".StartingPart.Activity.StartingActivity" />
    </application>

</manifest>